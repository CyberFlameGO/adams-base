adams.flow.control.Flow
	-annotation
	Allows the user to remove outliers interactively.
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.source.WekaClassifierSetup
					-classifier
						weka.classifiers.functions.LinearRegression
							-S
							0
							-R
							1.0E-8
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/bodyfat.arff
	-actor
		adams.flow.transformer.WekaFileReader
			-loader
				weka.core.converters.AArffLoader
	-actor
		adams.flow.transformer.WekaClassSelector
	-actor
		adams.flow.transformer.WekaCrossValidationEvaluator
			-output
				weka.classifiers.evaluation.output.prediction.Null
	-actor
		adams.flow.transformer.WekaPredictionsToSpreadSheet
	-actor
		adams.flow.control.RemoveOutliers
			-short-title
			true
			-stop-if-canceled
			true
			-detector
				adams.flow.control.removeoutliers.StdDev
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					original
					-actor
						adams.flow.control.ContainerValuePicker
							-value
							Original
							-switch-outputs
							true
					-actor
						adams.flow.sink.SpreadSheetDisplay
							-name
							original
							-short-title
							true
							-width
							400
							-writer
								adams.data.io.output.NullWriter
			-branch
				adams.flow.control.Sequence
					-name
					clean
					-actor
						adams.flow.control.ContainerValuePicker
							-value
							Clean
							-switch-outputs
							true
					-actor
						adams.flow.sink.SpreadSheetDisplay
							-name
							clean
							-short-title
							true
							-width
							400
							-x
							-2
							-writer
								adams.data.io.output.NullWriter
			-branch
				adams.flow.control.Sequence
					-name
					outliers
					-actor
						adams.flow.control.ContainerValuePicker
							-value
							Outliers
							-switch-outputs
							true
					-actor
						adams.flow.sink.SpreadSheetDisplay
							-name
							outliers
							-short-title
							true
							-width
							400
							-x
							-3
							-writer
								adams.data.io.output.NullWriter
	-flow-execution-listener
		adams.flow.execution.NullListener

