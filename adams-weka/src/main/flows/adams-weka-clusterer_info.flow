adams.flow.control.Flow
	-annotation
	Trains EM on the iris dataset (without class) and outputs model and number of clusters.
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.source.WekaClustererSetup
					-clusterer
						weka.clusterers.EM
							-I
							100
							-N
							-1
							-X
							10
							-max
							-1
							-ll-cv
							1.0E-6
							-ll-iter
							1.0E-6
							-M
							1.0E-6
							-K
							10
							-num-slots
							1
							-S
							100
			-actor
				adams.flow.sink.HistoryDisplay
					-width
					800
					-height
					600
					-entry-name-variable
					entry
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/iris.arff
	-actor
		adams.flow.transformer.WekaFileReader
			-loader
				weka.core.converters.AArffLoader
	-actor
		adams.flow.transformer.WekaFilter
			-filter
				weka.filters.unsupervised.attribute.Remove
					-R
					last
	-actor
		adams.flow.transformer.WekaTrainClusterer
			-post-processor
				adams.flow.transformer.wekaclusterer.PassThrough
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					model
					-actor
						adams.flow.transformer.SetVariable
							-var-name
							entry
							-var-value
							model
					-actor
						adams.flow.transformer.WekaClustererInfo
					-actor
						adams.flow.sink.CallableSink
							-callable
							HistoryDisplay
			-branch
				adams.flow.control.Sequence
					-name
					#clusters
					-actor
						adams.flow.transformer.SetVariable
							-var-name
							entry
							-var-value
							#clusters
					-actor
						adams.flow.transformer.WekaClustererInfo
							-type
							NUM_CLUSTERS
					-actor
						adams.flow.sink.CallableSink
							-callable
							HistoryDisplay
			-num-threads
			1
	-flow-execution-listener
		adams.flow.execution.NullListener

