adams.flow.control.Flow
	-annotation
	Trains a clusterer incrementally and outputs\nthe model every 25 instances.
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.source.WekaClustererSetup
					-clusterer
						weka.clusterers.Cobweb
							-A
							1.0
							-C
							0.0028209479177387815
							-S
							42
	-actor
		adams.flow.standalone.SetVariable
			-name
			reset counter
			-annotation
			The number of instances processed so far
			-var-name
			count
			-var-value
			0
	-actor
		adams.flow.standalone.SetVariable
			-name
			set interval
			-annotation
			The interval of training instances
			-var-name
			increment
			-var-value
			25
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/anneal_train.arff
	-actor
		adams.flow.transformer.WekaFileReader
			-loader
				weka.core.converters.ArffLoader
			-output-type
			INCREMENTAL
	-actor
		adams.flow.transformer.WekaFilter
			-filter
				weka.filters.unsupervised.attribute.Remove
					
					
					
	-actor
		adams.flow.transformer.WekaTrainClusterer
			-post-processor
				adams.flow.transformer.wekaclusterer.PassThrough
	-actor
		adams.flow.control.ConditionalTee
			-tee
				adams.flow.transformer.IncVariable
					-var-name
					count
					-inc-int
					@{increment}
			-tee
				adams.flow.control.ContainerValuePicker
					-tee
						adams.flow.sink.HistoryDisplay
							-caret-at-start
							true
							-entry-name-variable
							count
					-value
					Model
			-condition
				adams.flow.condition.bool.Counting
					-interval
					@{increment}
	-flow-execution-listener
		adams.flow.execution.NullListener

