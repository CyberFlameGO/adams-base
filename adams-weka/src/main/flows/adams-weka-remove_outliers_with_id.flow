adams.flow.control.Flow
	-annotation
	Allows the user to remove outliers interactively.\nLet\'s Weka generate CSV output in order to preserve\nthe ID attribute for identifying the rows correctly.\nThis CSV file is then the input for RemoveOutliers.\n
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.source.WekaClassifierSetup
					-classifier
						weka.classifiers.functions.LinearRegression
							-S
							0
							-R
							1.0E-8
	-actor
		adams.flow.source.Start
	-actor
		adams.flow.control.Trigger
			-name
			evaluate classifier
			-tee
				adams.flow.source.FileSupplier
					-file
					${EXAMPLE_FLOWS}/data/bodyfat.arff
			-tee
				adams.flow.control.Trigger
					-name
					tmp output file
					-tee
						adams.flow.source.FileSupplier
							-file
							${TMP}/preds.csv
					-tee
						adams.flow.transformer.Convert
							-conversion
								adams.data.conversion.MultiConversion
									-conversion
										adams.data.conversion.StringToFile
									-conversion
										adams.data.conversion.FileToString
											-absolute
											true
					-tee
						adams.flow.transformer.SetVariable
							-var-name
							outfile
			-tee
				adams.flow.transformer.WekaFileReader
					-loader
						weka.core.converters.AArffLoader
			-tee
				adams.flow.transformer.WekaClassSelector
			-tee
				adams.flow.transformer.WekaFilter
					-annotation
					for adding the ID attribute
					-filter
						weka.filters.unsupervised.attribute.AddID
							-C
							first
							-N
							ID
			-tee
				adams.flow.control.UpdateProperties
					-property
					output.outputFile
					-variable
					outfile
					-sub-actor
						adams.flow.transformer.WekaCrossValidationEvaluator
							-output
								weka.classifiers.evaluation.output.prediction.CSV
									-p
									1
	-actor
		adams.flow.control.Trigger
			-name
			remove outliers
			-tee
				adams.flow.source.Variable
					-var-name
					outfile
			-tee
				adams.flow.transformer.SpreadSheetFileReader
					-reader
						adams.data.io.input.CsvSpreadSheetReader
							-data-row-type
								adams.data.spreadsheet.DenseDataRow
							-spreadsheet-type
								adams.data.spreadsheet.SpreadSheet
			-tee
				adams.flow.control.RemoveOutliers
					-short-title
					true
					-stop-if-canceled
					true
					-detector
						adams.flow.control.removeoutliers.StdDev
					-col-actual
					2
					-col-predicted
					3
			-tee
				adams.flow.control.Branch
					-branch
						adams.flow.control.Sequence
							-name
							original
							-actor
								adams.flow.control.ContainerValuePicker
									-value
									Original
									-switch-outputs
									true
							-actor
								adams.flow.sink.SpreadSheetDisplay
									-name
									original
									-short-title
									true
									-width
									400
									-writer
										adams.data.io.output.NullWriter
					-branch
						adams.flow.control.Sequence
							-name
							clean
							-actor
								adams.flow.control.ContainerValuePicker
									-value
									Clean
									-switch-outputs
									true
							-actor
								adams.flow.sink.SpreadSheetDisplay
									-name
									clean
									-short-title
									true
									-width
									400
									-x
									-2
									-writer
										adams.data.io.output.NullWriter
					-branch
						adams.flow.control.Sequence
							-name
							outliers
							-actor
								adams.flow.control.ContainerValuePicker
									-value
									Outliers
									-switch-outputs
									true
							-actor
								adams.flow.sink.SpreadSheetDisplay
									-name
									outliers
									-short-title
									true
									-width
									400
									-x
									-3
									-writer
										adams.data.io.output.NullWriter
	-flow-execution-listener
		adams.flow.execution.NullListener

