adams.flow.control.Flow
	-annotation
	Trains J48 on iris dataset and outputs model, tree as digraph and and source code.
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.source.WekaClassifierSetup
					-classifier
						weka.classifiers.trees.J48
							-C
							0.25
							-M
							2
			-actor
				adams.flow.sink.HistoryDisplay
					-width
					800
					-height
					600
					-entry-name-variable
					entry
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/iris.arff
	-actor
		adams.flow.transformer.WekaFileReader
			-loader
				weka.core.converters.AArffLoader
	-actor
		adams.flow.transformer.WekaClassSelector
	-actor
		adams.flow.transformer.WekaTrainClassifier
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					model
					-actor
						adams.flow.transformer.SetVariable
							-var-name
							entry
							-var-value
							model
					-actor
						adams.flow.transformer.WekaClassifierInfo
					-actor
						adams.flow.sink.CallableSink
							-callable
							HistoryDisplay
			-branch
				adams.flow.control.Sequence
					-name
					graph
					-actor
						adams.flow.transformer.SetVariable
							-var-name
							entry
							-var-value
							graph
					-actor
						adams.flow.transformer.WekaClassifierInfo
							-type
							GRAPH
					-actor
						adams.flow.sink.CallableSink
							-callable
							HistoryDisplay
			-branch
				adams.flow.control.Sequence
					-name
					sourcecode
					-actor
						adams.flow.transformer.SetVariable
							-var-name
							entry
							-var-value
							sourcecode
					-actor
						adams.flow.transformer.WekaClassifierInfo
							-type
							SOURCE_CODE
					-actor
						adams.flow.sink.CallableSink
							-callable
							HistoryDisplay
			-num-threads
			1
	-flow-execution-listener
		adams.flow.execution.NullListener

