adams.flow.control.Flow -annotation "Generates a data stream using the Agrawal generator\\nwith 200,000 examples, evaluates the DecisionStump\\nclassifier every 1000 and displays the result of the\\nevaluation in textual format and the statistics in \\nsequence plots." -flow-execution-listener adams.flow.execution.NullListener
 adams.flow.standalone.GridView -height 800 -num-rows 3 -writer adams.gui.print.NullWriter
  adams.flow.sink.SequencePlotter -name Kappa -x -3 -writer adams.gui.print.NullWriter -paintlet adams.gui.visualization.sequence.LinePaintlet -overlay-paintlet adams.gui.visualization.sequence.CirclePaintlet -marker-paintlet adams.flow.sink.sequenceplotter.NoMarkers -error-paintlet adams.flow.sink.sequenceplotter.NoErrorPaintlet -mouse-click-action adams.flow.sink.sequenceplotter.NullClickAction -color-provider adams.gui.visualization.core.DefaultColorProvider -overlay-color-provider adams.gui.visualization.core.DefaultColorProvider -title Kappa -axis-x "adams.gui.visualization.core.AxisPanelOptions -label \"Evaluation step\" -tick-generator adams.gui.visualization.core.axis.FancyTickGenerator -nth-value 2 -width 40 -custom-format 0" -axis-y "adams.gui.visualization.core.AxisPanelOptions -label Statistic -tick-generator adams.gui.visualization.core.axis.FancyTickGenerator -nth-value 1 -width 60 -custom-format 0.0" -plot-updater adams.flow.sink.sequenceplotter.SimplePlotUpdater -post-processor adams.flow.sink.sequenceplotter.PassThrough
  adams.flow.sink.SequencePlotter -name "Percent correct" -x -3 -writer adams.gui.print.NullWriter -paintlet adams.gui.visualization.sequence.LinePaintlet -overlay-paintlet adams.gui.visualization.sequence.CirclePaintlet -marker-paintlet adams.flow.sink.sequenceplotter.NoMarkers -error-paintlet adams.flow.sink.sequenceplotter.NoErrorPaintlet -mouse-click-action adams.flow.sink.sequenceplotter.NullClickAction -color-provider adams.gui.visualization.core.DefaultColorProvider -overlay-color-provider adams.gui.visualization.core.DefaultColorProvider -title "Percent correct" -axis-x "adams.gui.visualization.core.AxisPanelOptions -label \"Evaluation step\" -tick-generator adams.gui.visualization.core.axis.FancyTickGenerator -nth-value 2 -width 40 -custom-format 0" -axis-y "adams.gui.visualization.core.AxisPanelOptions -label Statistic -tick-generator adams.gui.visualization.core.axis.FancyTickGenerator -nth-value 2 -width 60 -custom-format 0.0" -plot-updater adams.flow.sink.sequenceplotter.SimplePlotUpdater -post-processor adams.flow.sink.sequenceplotter.PassThrough
  adams.flow.sink.Display -always-clear true -writer adams.data.io.output.NullWriter
 adams.flow.standalone.CallableActors
  adams.flow.source.MOAClassifierSetup
 adams.flow.source.MOAStream -num-examples 200000 -chunk-size 1000
 adams.flow.transformer.MOAClassifierEvaluation -classifier MOAClassifierSetup -output-interval 100
 adams.flow.control.Branch
  adams.flow.control.Sequence -name text
   adams.flow.transformer.MOALearningEvaluation
   adams.flow.sink.CallableSink -callable Display
  adams.flow.control.Sequence -name kappa
   adams.flow.transformer.MOAMeasurementsFilter -regexp "Kappa St.*"
   adams.flow.transformer.MOAMeasurementPlotGenerator
   adams.flow.sink.CallableSink -callable Kappa
  adams.flow.control.Sequence -name "percent correct"
   adams.flow.transformer.MOAMeasurementsFilter -regexp .*correct.*
   adams.flow.transformer.MOAMeasurementPlotGenerator
   adams.flow.sink.CallableSink -callable "Percent correct"
