adams.flow.control.Flow -annotation "Generates a data stream using the Random tree generator\\nwith 200,000 examples, evaluates the FIMTDD\\nregressor every 1000 and displays the result of the\\nevaluation in textual format and the statistics in \\nsequence plots." -flow-execution-listener adams.flow.execution.NullListener
 adams.flow.standalone.GridView -height 800 -num-rows 3 -writer adams.gui.print.NullWriter
  adams.flow.sink.SequencePlotter -name MAE -x -3 -writer adams.gui.print.NullWriter -paintlet adams.gui.visualization.sequence.LinePaintlet -overlay-paintlet adams.gui.visualization.sequence.CirclePaintlet -marker-paintlet adams.flow.sink.sequenceplotter.NoMarkers -error-paintlet adams.flow.sink.sequenceplotter.NoErrorPaintlet -mouse-click-action adams.flow.sink.sequenceplotter.NullClickAction -color-provider adams.gui.visualization.core.DefaultColorProvider -overlay-color-provider adams.gui.visualization.core.DefaultColorProvider -title "Mean absolute error" -axis-x "adams.gui.visualization.core.AxisPanelOptions -label \"Evaluation step\" -tick-generator adams.gui.visualization.core.axis.FancyTickGenerator -nth-value 2 -width 40 -custom-format 0" -axis-y "adams.gui.visualization.core.AxisPanelOptions -label Statistic -tick-generator adams.gui.visualization.core.axis.FancyTickGenerator -nth-value 2 -width 60 -custom-format 0.000" -plot-updater adams.flow.sink.sequenceplotter.SimplePlotUpdater -post-processor adams.flow.sink.sequenceplotter.PassThrough
  adams.flow.sink.SequencePlotter -name RMSE -x -3 -writer adams.gui.print.NullWriter -paintlet adams.gui.visualization.sequence.LinePaintlet -overlay-paintlet adams.gui.visualization.sequence.CirclePaintlet -marker-paintlet adams.flow.sink.sequenceplotter.NoMarkers -error-paintlet adams.flow.sink.sequenceplotter.NoErrorPaintlet -mouse-click-action adams.flow.sink.sequenceplotter.NullClickAction -color-provider adams.gui.visualization.core.DefaultColorProvider -overlay-color-provider adams.gui.visualization.core.DefaultColorProvider -title "Root mean squared error" -axis-x "adams.gui.visualization.core.AxisPanelOptions -label \"Evaluation step\" -tick-generator adams.gui.visualization.core.axis.FancyTickGenerator -nth-value 2 -width 40 -custom-format 0" -axis-y "adams.gui.visualization.core.AxisPanelOptions -label Statistic -tick-generator adams.gui.visualization.core.axis.FancyTickGenerator -nth-value 2 -width 60 -custom-format 0.0000" -plot-updater adams.flow.sink.sequenceplotter.SimplePlotUpdater -post-processor adams.flow.sink.sequenceplotter.PassThrough
  adams.flow.sink.Display -always-clear true -writer adams.data.io.output.NullWriter
 adams.flow.standalone.CallableActors
  adams.flow.source.MOARegressorSetup
 adams.flow.source.MOAStream -stream moa.streams.generators.RandomTreeGenerator -num-examples 200000 -chunk-size 1000
 adams.flow.transformer.MOARegressorEvaluation -regressor MOARegressorSetup -output-interval 100
 adams.flow.control.Branch
  adams.flow.control.Sequence -name text
   adams.flow.transformer.MOALearningEvaluation
   adams.flow.sink.CallableSink -callable Display
  adams.flow.control.Sequence -name MAE
   adams.flow.transformer.MOAMeasurementsFilter -regexp "mean absolute error.*"
   adams.flow.transformer.MOAMeasurementPlotGenerator
   adams.flow.sink.CallableSink -callable MAE
  adams.flow.control.Sequence -name RMSE
   adams.flow.transformer.MOAMeasurementsFilter -regexp "root mean squared error.*"
   adams.flow.transformer.MOAMeasurementPlotGenerator
   adams.flow.sink.CallableSink -callable RMSE
