adams.flow.control.Flow
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.source.SequenceSource
					-name
					model
					-actor
						adams.flow.source.FileSupplier
							-file
							${EXAMPLE_FLOWS}/data/fimtdd.model
					-actor
						adams.flow.transformer.MOAModelReader
							-output-only-model
							true
	-actor
		adams.flow.source.Start
	-actor
		adams.flow.control.Trigger
			-name
			serialized model
			-annotation
			This flow uses a serialized model to make predictions on a \nrandom tree stream and output the generated regression.
			-tee
				adams.flow.source.MOAStream
					-stream
					moa.streams.generators.RandomTreeGenerator
					-num-examples
					100
			-tee
				adams.flow.transformer.MOARegressing
					-model
					${EXAMPLE_FLOWS}/data/fimtdd.model
					-on-the-fly
					true
			-tee
				adams.flow.control.ContainerValuePicker
					-tee
						adams.flow.sink.Display
							-name
							Display regression (serialized)
							-short-title
							true
							-writer
								adams.data.io.output.NullWriter
	-actor
		adams.flow.control.Trigger
			-name
			callable actor
			-annotation
			This flow uses a model obtained from a callable actor to make \npredictions on a random tree stream and output the \ngenerated regression (the callable actor merely loads a\nserialized model in this example).
			-tee
				adams.flow.source.MOAStream
					-stream
					moa.streams.generators.RandomTreeGenerator
					-num-examples
					100
			-tee
				adams.flow.transformer.MOARegressing
					-model-actor
					model
					-on-the-fly
					true
			-tee
				adams.flow.control.ContainerValuePicker
					-tee
						adams.flow.sink.Display
							-name
							Display regression (model)
							-short-title
							true
							-x
							-3
							-writer
								adams.data.io.output.NullWriter
	-flow-execution-listener
		adams.flow.execution.NullListener

