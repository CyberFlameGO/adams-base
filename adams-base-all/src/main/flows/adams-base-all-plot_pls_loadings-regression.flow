adams.flow.control.Flow
	-annotation
	Evaluates a PLSClassifier on a dataset and plots loadings and errors.
	-actor
		adams.flow.standalone.GlobalActors
			-actor
				adams.flow.source.WekaClassifierSetup
					-classifier
						weka.classifiers.functions.PLSClassifierWeightedWithLoadings
							-filter
							weka.filters.supervised.attribute.PLSFilterWithLoadings -C 5 -M -A PLS1 -P center
			-actor
				adams.flow.sink.SequencePlotter
					-name
					Errors
					-writer
						adams.gui.print.NullWriter
					-paintlet
						adams.gui.visualization.sequence.CirclePaintlet
					-marker-paintlet
						adams.flow.sink.sequenceplotter.NoMarkers
					-error-paintlet
						adams.flow.sink.sequenceplotter.NoErrorPaintlet
					-color-provider
						adams.gui.visualization.core.DefaultColorProvider
					-title
					Errors
					-axis-x
						adams.gui.visualization.core.AxisPanelOptions
							-label
							actual
							-tick-generator
								adams.gui.visualization.core.axis.SimpleTickGenerator
							-width
							40
							-custom-format
							0.000
					-axis-y
						adams.gui.visualization.core.AxisPanelOptions
							-label
							predicted
							-tick-generator
								adams.gui.visualization.core.axis.SimpleTickGenerator
							-width
							80
							-custom-format
							0.000
					-plot-updater
						adams.flow.sink.sequenceplotter.SimplePlotUpdater
							-update-interval
							-1
					-post-processor
						adams.flow.sink.sequenceplotter.PassThrough
			-actor
				adams.flow.sink.SequencePlotter
					-name
					Loadings
					-x
					-3
					-writer
						adams.gui.print.NullWriter
					-paintlet
						adams.gui.visualization.sequence.LinePaintlet
					-marker-paintlet
						adams.flow.sink.sequenceplotter.NoMarkers
					-error-paintlet
						adams.flow.sink.sequenceplotter.NoErrorPaintlet
					-color-provider
						adams.gui.visualization.core.DefaultColorProvider
					-title
					Loadings
					-axis-x
						adams.gui.visualization.core.AxisPanelOptions
							-label
							attribute index
							-tick-generator
								adams.gui.visualization.core.axis.SimpleTickGenerator
							-width
							40
							-custom-format
							0
					-axis-y
						adams.gui.visualization.core.AxisPanelOptions
							-label
							loading
							-tick-generator
								adams.gui.visualization.core.axis.SimpleTickGenerator
							-width
							80
							-custom-format
							0.000
					-plot-updater
						adams.flow.sink.sequenceplotter.SimplePlotUpdater
							-update-interval
							-1
					-post-processor
						adams.flow.sink.sequenceplotter.PassThrough
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/triazines.arff
	-actor
		adams.flow.transformer.WekaFileReader
			-loader
				weka.core.converters.ArffLoader
	-actor
		adams.flow.transformer.WekaClassSelector
	-actor
		adams.flow.transformer.WekaRandomSplit
	-actor
		adams.flow.transformer.WekaTrainTestSetEvaluator
			-output
				weka.classifiers.evaluation.output.prediction.Null
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					plot errors
					-actor
						adams.flow.control.ContainerValuePicker
							-value
							Evaluation
							-switch-outputs
							true
					-actor
						adams.flow.transformer.WekaPredictionsToInstances
					-actor
						adams.flow.transformer.Convert
							-conversion
								adams.data.conversion.WekaInstancesToSpreadSheet
					-actor
						adams.flow.transformer.SpreadSheetPlotGenerator
							-generator
								adams.flow.transformer.plotgenerator.XYPlotGenerator
									-default-cell-value
									0.0
									-columns
									2
									-x-column
									1
					-actor
						adams.flow.sink.GlobalSink
							-global
							Errors
			-branch
				adams.flow.control.Sequence
					-name
					plot loadings
					-actor
						adams.flow.control.ContainerValuePicker
							-value
							Model
							-switch-outputs
							true
					-actor
						adams.flow.transformer.WekaExtractPLSMatrix
					-actor
						adams.flow.transformer.SpreadSheetPlotGenerator
							-generator
								adams.flow.transformer.plotgenerator.SimplePlotGenerator
									-default-cell-value
									0.0
									-columns
									first-last
					-actor
						adams.flow.sink.GlobalSink
							-global
							Loadings
	-flow-execution-listener
		adams.flow.execution.NullListener

