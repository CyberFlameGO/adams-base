adams.flow.control.Flow
	-annotation
	Generates a Xbar R chart from the samples.\nData taken from here:\nhttp://www.wikihow.com/Sample/Control-Chart-for-Multiple-Groups
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/subgroups.csv
	-actor
		adams.flow.transformer.SpreadSheetFileReader
			-reader
				adams.data.io.input.CsvSpreadSheetReader
					-data-row-type
						adams.data.spreadsheet.DenseDataRow
					-spreadsheet-type
						adams.data.spreadsheet.SpreadSheet
					-separator
					\t
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					control chart
					-actor
						adams.flow.transformer.SpreadSheetColumnFilter
							-finder
								adams.data.spreadsheet.columnfinder.ByIndex
									-columns
									2-last
					-actor
						adams.flow.transformer.Convert
							-conversion
								adams.data.conversion.SpreadSheetToDoubleMatrix
					-actor
						adams.flow.transformer.ControlChart
							-chart
								adams.data.spc.XBarRChart
							-violation-finder
								adams.data.spc.BeyondLimitsViolations
					-actor
						adams.flow.sink.ControlChartPlot
							-short-title
							true
							-writer
								adams.gui.print.NullWriter
							-paintlet
								adams.flow.sink.controlchartplot.ChartPaintlet
							-limit-paintlet
								adams.flow.sink.controlchartplot.LimitPaintlet
							-mouse-click-action
								adams.flow.sink.sequenceplotter.NullClickAction
							-color-provider
								adams.gui.visualization.core.DefaultColorProvider
							-axis-x
								adams.gui.visualization.core.AxisPanelOptions
									-label
									sample
									-tick-generator
										adams.gui.visualization.core.axis.FancyTickGenerator
											-num-ticks
											20
									-nth-value
									1
									-width
									40
									-top-margin
									0.05
									-bottom-margin
									0.05
									-custom-format
									0
							-axis-y
								adams.gui.visualization.core.AxisPanelOptions
									-label
									y
									-tick-generator
										adams.gui.visualization.core.axis.FancyTickGenerator
									-nth-value
									2
									-width
									60
									-top-margin
									0.05
									-bottom-margin
									0.05
									-custom-format
									0.0
			-branch
				adams.flow.sink.SpreadSheetDisplay
					-name
					Raw
					-short-title
					true
					-width
					400
					-x
					-3
					-writer
						adams.data.io.output.NullWriter
	-flow-execution-listener
		adams.flow.execution.NullListener

