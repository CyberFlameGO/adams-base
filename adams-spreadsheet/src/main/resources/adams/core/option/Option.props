# Configuration file for registering custom hooks for objects that special
# constructors and string representations.
#
# Format:
# An Option needs three entries:
# 1. whether the hook is enabled ("classname=true|false"), e.g.:
#    java.io.File=true
# 2. the "hook" for turning the object into a string ("classname#method"), e.g.:
#    java.io.File#toString=adams.core.option.parsing.FileParsing#toString
# 3. the "hook" for creating an object from the string ("classname#method"), e.g.:
#    java.io.File#valueOf=adams.core.option.parsing.FileParsing#valueOf
#
# Other options are stored in here as well, but they must be prefixed with
# "option.".
#
#
# Author: FracPete (fracpete at waikato dot ac dot nz)

# SpreadSheetColumnIndex objects
adams.data.spreadsheet.SpreadSheetColumnIndex=true
adams.data.spreadsheet.SpreadSheetColumnIndex#toString=adams.core.option.parsing.SpreadSheetColumnIndexParsing#toString
adams.data.spreadsheet.SpreadSheetColumnIndex#valueOf=adams.core.option.parsing.SpreadSheetColumnIndexParsing#valueOf

# SpreadSheetColumnRange objects
adams.data.spreadsheet.SpreadSheetColumnRange=true
adams.data.spreadsheet.SpreadSheetColumnRange#toString=adams.core.option.parsing.SpreadSheetColumnRangeEditor#toString
adams.data.spreadsheet.SpreadSheetColumnRange#valueOf=adams.core.option.parsing.SpreadSheetColumnRangeEditor#valueOf

# SpreadSheetUnorderedColumnRange objects
adams.data.spreadsheet.SpreadSheetUnorderedColumnRange=true
adams.data.spreadsheet.SpreadSheetUnorderedColumnRange#toString=adams.core.option.parsing.SpreadSheetUnorderedColumnRangeParsing#toString
adams.data.spreadsheet.SpreadSheetUnorderedColumnRange#valueOf=adams.core.option.parsing.SpreadSheetUnorderedColumnRangeParsing#valueOf

# SpreadSheetQueryText objects
adams.parser.SpreadSheetQueryText=true
adams.parser.SpreadSheetQueryText#toString=adams.core.option.parsing.SimpleScriptParsing#toString
adams.parser.SpreadSheetQueryText#valueOf=adams.core.option.parsing.SimpleScriptParsing#valueOf

# LookUpUpdateText objects
adams.parser.LookUpUpdateText=true
adams.parser.LookUpUpdateText#toString=adams.core.option.parsing.SimpleScriptParsing#toString
adams.parser.LookUpUpdateText#valueOf=adams.core.option.parsing.SimpleScriptParsing#valueOf
