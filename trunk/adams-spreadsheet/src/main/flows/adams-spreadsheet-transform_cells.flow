adams.flow.control.Flow
	-annotation
	This flows applies a global MathExpression transformer to all the\ncells in columns 4 and 7. The original and the transformer\nspreadsheet are displayed side-by-side.
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.transformer.MathExpression
					-expression
					rint(X)
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/bolts.csv
	-actor
		adams.flow.transformer.SpreadSheetFileReader
			-reader
				adams.data.io.input.CsvSpreadSheetReader
					-spreadsheet-type
						adams.data.spreadsheet.SpreadSheet
	-actor
		adams.flow.control.Tee
			-tee
				adams.flow.sink.SpreadSheetDisplay
					-name
					original
					-short-title
					true
	-actor
		adams.flow.transformer.SpreadSheetTransformCells
			-finder
				adams.data.spreadsheet.cellfinder.CellRange
					-columns
					4,7
			-transformer
			MathExpression
	-actor
		adams.flow.sink.SpreadSheetDisplay
			-name
			rounded
			-short-title
			true
			-x
			-3
	-flow-execution-listener
		adams.flow.execution.NullListener

