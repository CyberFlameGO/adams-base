adams.flow.control.Flow
	-annotation
	Generates a data stream using the Agrawal generator\nwith 200,000 examples, evaluates the DecisionStump\nclassifier every 1000 and displays the result of the\nevaluation in textual format and the statistics in \nsequence plots.
	-actor
		adams.flow.standalone.GridView
			-height
			800
			-actor
				adams.flow.sink.SequencePlotter
					-name
					Kappa
					-x
					-3
					-writer
						adams.gui.print.NullWriter
					-paintlet
						adams.gui.visualization.sequence.LinePaintlet
					-overlay-paintlet
						adams.gui.visualization.sequence.CirclePaintlet
					-marker-paintlet
						adams.flow.sink.sequenceplotter.NoMarkers
					-error-paintlet
						adams.flow.sink.sequenceplotter.NoErrorPaintlet
					-color-provider
						adams.gui.visualization.core.DefaultColorProvider
					-overlay-color-provider
						adams.gui.visualization.core.DefaultColorProvider
					-title
					Kappa
					-axis-x
						adams.gui.visualization.core.AxisPanelOptions
							-label
							Evaluation step
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
							-nth-value
							2
							-width
							40
							-custom-format
							0
					-axis-y
						adams.gui.visualization.core.AxisPanelOptions
							-label
							Statistic
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
							-nth-value
							1
							-width
							60
							-custom-format
							0.0
					-plot-updater
						adams.flow.sink.sequenceplotter.SimplePlotUpdater
					-post-processor
						adams.flow.sink.sequenceplotter.PassThrough
			-actor
				adams.flow.sink.SequencePlotter
					-name
					Percent correct
					-x
					-3
					-writer
						adams.gui.print.NullWriter
					-paintlet
						adams.gui.visualization.sequence.LinePaintlet
					-overlay-paintlet
						adams.gui.visualization.sequence.CirclePaintlet
					-marker-paintlet
						adams.flow.sink.sequenceplotter.NoMarkers
					-error-paintlet
						adams.flow.sink.sequenceplotter.NoErrorPaintlet
					-color-provider
						adams.gui.visualization.core.DefaultColorProvider
					-overlay-color-provider
						adams.gui.visualization.core.DefaultColorProvider
					-title
					Percent correct
					-axis-x
						adams.gui.visualization.core.AxisPanelOptions
							-label
							Evaluation step
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
							-nth-value
							2
							-width
							40
							-custom-format
							0
					-axis-y
						adams.gui.visualization.core.AxisPanelOptions
							-label
							Statistic
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
							-nth-value
							2
							-width
							60
							-custom-format
							0.0
					-plot-updater
						adams.flow.sink.sequenceplotter.SimplePlotUpdater
					-post-processor
						adams.flow.sink.sequenceplotter.PassThrough
			-actor
				adams.flow.sink.Display
					-always-clear
					true
			-num-rows
			3
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.source.MOAClassifierSetup
	-actor
		adams.flow.source.MOAStream
			-num-examples
			200000
			-chunk-size
			1000
	-actor
		adams.flow.transformer.MOAClassifierEvaluation
			-classifier
			MOAClassifierSetup
			-output-interval
			100
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					text
					-actor
						adams.flow.transformer.MOALearningEvaluation
					-actor
						adams.flow.sink.CallableSink
							-callable
							Display
			-branch
				adams.flow.control.Sequence
					-name
					kappa
					-actor
						adams.flow.transformer.MOAMeasurementsFilter
							-regexp
							Kappa St.*
					-actor
						adams.flow.transformer.MOAMeasurementPlotGenerator
					-actor
						adams.flow.sink.CallableSink
							-callable
							Kappa
			-branch
				adams.flow.control.Sequence
					-name
					percent correct
					-actor
						adams.flow.transformer.MOAMeasurementsFilter
							-regexp
							.*correct.*
					-actor
						adams.flow.transformer.MOAMeasurementPlotGenerator
					-actor
						adams.flow.sink.CallableSink
							-callable
							Percent correct
	-flow-execution-listener
		adams.flow.execution.NullListener

