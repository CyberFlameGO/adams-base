adams.flow.control.Flow
	-annotation
	Builds a SimpleKMeans clusterer on a training set\nand evaluates it against a test set, outputting the\nresults of the evaluation.
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.source.SequenceSource
					-name
					train
					-actor
						adams.flow.source.FileSupplier
							-file
							${EXAMPLE_FLOWS}/data/anneal_train.arff
					-actor
						adams.flow.transformer.WekaFileReader
							-loader
								weka.core.converters.ArffLoader
					-actor
						adams.flow.transformer.WekaFilter
							-filter
								weka.filters.unsupervised.attribute.Remove
									-R
									last
									
			-actor
				adams.flow.source.SequenceSource
					-name
					test
					-actor
						adams.flow.source.FileSupplier
							-file
							${EXAMPLE_FLOWS}/data/anneal_test.arff
					-actor
						adams.flow.transformer.WekaFileReader
							-loader
								weka.core.converters.ArffLoader
					-actor
						adams.flow.transformer.WekaFilter
							-filter
								weka.filters.unsupervised.attribute.Remove
									-R
									last
									
			-actor
				adams.flow.source.WekaClustererSetup
					-clusterer
						weka.clusterers.SimpleKMeans
							-N
							2
							-A
							weka.core.EuclideanDistance -R first-last
							-I
							500
							-num-slots
							1
							-S
							10
	-actor
		adams.flow.source.MakeContainer
			-callable-actor
			train
			-callable-actor
			test
			-value-name
			Train
			-value-name
			Test
			-container-class
				adams.flow.container.WekaTrainTestSetContainer
	-actor
		adams.flow.transformer.WekaTrainTestSetClustererEvaluator
			-output-model
			true
	-actor
		adams.flow.transformer.WekaClusterEvaluationSummary
	-actor
		adams.flow.sink.Display
	-flow-execution-listener
		adams.flow.execution.NullListener

