adams.flow.control.Flow
	-annotation
	Extracts a random subset of rows from a text file.
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/numbers.txt
	-actor
		adams.flow.transformer.TextFileReader
			-reader
				adams.data.io.input.LineArrayTextReader
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					normal
					-actor
						adams.flow.transformer.ArraySubSample
							-size
							30.0
							-generator
								adams.data.random.JavaRandomInt
					-actor
						adams.flow.transformer.ArrayToSequence
					-actor
						adams.flow.sink.Display
							-name
							Normal
							-short-title
							true
							-writer
								adams.data.io.output.NullWriter
			-branch
				adams.flow.control.Sequence
					-name
					inverted
					-actor
						adams.flow.transformer.ArraySubSample
							-size
							30.0
							-generator
								adams.data.random.JavaRandomInt
							-split-result
							INVERSE
					-actor
						adams.flow.transformer.ArrayToSequence
					-actor
						adams.flow.sink.Display
							-name
							Inverted
							-short-title
							true
							-x
							-3
							-writer
								adams.data.io.output.NullWriter
	-flow-execution-listener
		adams.flow.execution.NullListener

