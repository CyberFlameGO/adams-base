adams.flow.control.Flow
	-annotation
	Demonstrates the use of plot-processors when generating timeseries plots.\nIn this flow, the raw data is plotted next to data smoothed using LOWESS \nand Savitzky-Golay.
	-actor
		adams.flow.standalone.TabView
			-actor
				adams.flow.sink.TimeseriesDisplay
					-name
					original
					-writer
						adams.gui.print.NullWriter
					-axis-x
						adams.gui.visualization.timeseries.DefaultTimeseriesXAxisPanelOptions
							-label
							time
							-type
							DATE
							-tick-generator
								adams.gui.visualization.core.axis.PeriodicityTickGenerator
							-nth-value
							2
							-width
							40
							-periodicity
							YEARLY
					-axis-y
						adams.gui.visualization.timeseries.DefaultTimeseriesYAxisPanelOptions
							-label
							value
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
							-nth-value
							2
							-width
							60
							-custom-format
							0.0
					-color-provider
						adams.gui.visualization.core.DefaultColorProvider
			-actor
				adams.flow.sink.TimeseriesDisplay
					-name
					fft
					-writer
						adams.gui.print.NullWriter
					-axis-x
						adams.gui.visualization.timeseries.DefaultTimeseriesXAxisPanelOptions
							-label
							time
							-type
							TIME
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
							-nth-value
							2
							-width
							40
							-custom-format
							mm:ss
					-axis-y
						adams.gui.visualization.timeseries.DefaultTimeseriesYAxisPanelOptions
							-label
							value
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
							-nth-value
							2
							-width
							60
							-custom-format
							0.0
					-color-provider
						adams.gui.visualization.core.DefaultColorProvider
			-actor
				adams.flow.sink.TimeseriesDisplay
					-name
					inv fft
					-writer
						adams.gui.print.NullWriter
					-axis-x
						adams.gui.visualization.timeseries.DefaultTimeseriesXAxisPanelOptions
							-label
							time
							-type
							TIME
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
							-nth-value
							2
							-width
							40
							-custom-format
							mm:ss
					-axis-y
						adams.gui.visualization.timeseries.DefaultTimeseriesYAxisPanelOptions
							-label
							value
							-tick-generator
								adams.gui.visualization.core.axis.FancyTickGenerator
							-nth-value
							2
							-width
							60
							-custom-format
							0.0
					-color-provider
						adams.gui.visualization.core.DefaultColorProvider
	-actor
		adams.flow.standalone.SetVariable
			-var-name
			type
			-var-value
			Fortified
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/wine.arff
	-actor
		adams.flow.transformer.TimeseriesFileReader
			-reader
				adams.data.io.input.SpreadSheetTimeseriesReader
					-create-dummy-report
					true
					-reader
						adams.data.io.input.ArffSpreadSheetReader
							-data-row-type
								adams.data.spreadsheet.DenseDataRow
							-spreadsheet-type
								adams.data.spreadsheet.SpreadSheet
					-column-timestamp
					Date
					-column-value
					@{type}
	-actor
		adams.flow.transformer.SetID
			-id
			@{type}
	-actor
		adams.flow.control.Cast
			-classname
			adams.data.timeseries.Timeseries
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					original
					-actor
						adams.flow.sink.CallableSink
							-callable
							original
			-branch
				adams.flow.control.Sequence
					-name
					fft real
					-actor
						adams.flow.transformer.TimeseriesFilter
							-filter
								adams.data.filter.FFT
					-actor
						adams.flow.transformer.SetID
							-id
							fft real
					-actor
						adams.flow.control.Cast
							-classname
							adams.data.timeseries.Timeseries
					-actor
						adams.flow.sink.CallableSink
							-callable
							fft
			-branch
				adams.flow.control.Sequence
					-name
					fft complex
					-actor
						adams.flow.transformer.TimeseriesFilter
							-filter
								adams.data.filter.FFT
									-real
									false
					-actor
						adams.flow.transformer.SetID
							-id
							fft complex
					-actor
						adams.flow.control.Cast
							-classname
							adams.data.timeseries.Timeseries
					-actor
						adams.flow.sink.CallableSink
							-callable
							fft
			-branch
				adams.flow.control.Sequence
					-name
					inv fft real
					-actor
						adams.flow.transformer.TimeseriesFilter
							-filter
								adams.data.filter.FFT
									-inverse
									true
					-actor
						adams.flow.transformer.SetID
							-id
							inv fft real
					-actor
						adams.flow.control.Cast
							-classname
							adams.data.timeseries.Timeseries
					-actor
						adams.flow.sink.CallableSink
							-callable
							inv fft
			-branch
				adams.flow.control.Sequence
					-name
					inv fft complex
					-actor
						adams.flow.transformer.TimeseriesFilter
							-filter
								adams.data.filter.FFT
									-inverse
									true
									-real
									false
					-actor
						adams.flow.transformer.SetID
							-id
							inv fft complex
					-actor
						adams.flow.control.Cast
							-classname
							adams.data.timeseries.Timeseries
					-actor
						adams.flow.sink.CallableSink
							-callable
							inv fft
			-num-threads
			1
	-flow-execution-listener
		adams.flow.execution.NullListener

