--> dumpfile.txt
adams.flow.control.Flow
	-actor
		adams.flow.source.NewFlow
			-template
				adams.flow.template.PromptUser
	-actor
		adams.flow.transformer.OptionProducer
			-producer
				adams.core.option.NestedProducer
					-logging-level
					OFF
					-output-classpath
					false
					-output-line-numbers
					false
					-output-prolog
					true
					-output-variable-values
					false
	-actor
		adams.flow.transformer.StringSplit
			-expression
			\n
	-actor
		adams.flow.transformer.StringMatcher
			-regexp
			#.*
			-invert
			true
	-actor
		adams.flow.transformer.StringJoin
			-glue
			\n
	-actor
		adams.flow.sink.DumpFile
			-output
			${TMP}/dumpfile.txt
	-flow-execution-listener
		adams.flow.execution.NullListener

