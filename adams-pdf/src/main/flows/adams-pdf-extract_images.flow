adams.flow.control.Flow -annotation "Extracts all the images from a PDF and displays them." -flow-execution-listener adams.flow.execution.NullListener
 adams.flow.source.FileSupplier -file ${EXAMPLE_FLOWS}/data/multiple_images.pdf
 adams.flow.transformer.PDFExtractImages
 adams.flow.sink.DisplayPanelManager -width 800 -height 600 -provider "adams.flow.sink.ImageViewer -writer adams.gui.print.NullWriter -selection-processor adams.gui.visualization.image.selection.NullProcessor -image-overlay adams.gui.visualization.image.NullOverlay"
