adams.flow.control.Flow
	-annotation
	Computes the median of several images.
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.sink.DisplayPanelManager
					-provider
						adams.flow.sink.ImageViewer
							-writer
								adams.gui.print.NullWriter
							-selection-processor
								adams.gui.visualization.image.selection.NullProcessor
							-image-overlay
								adams.gui.visualization.image.NullOverlay
					-entry-name-variable
					name
	-actor
		adams.flow.source.FileSupplier
			-output-array
			true
			-file
			${EXAMPLE_FLOWS}/data/text1.png
			-file
			${EXAMPLE_FLOWS}/data/text2.png
			-file
			${EXAMPLE_FLOWS}/data/text3.png
			-file
			${EXAMPLE_FLOWS}/data/text4.png
	-actor
		adams.flow.control.ArrayProcess
			-actor
				adams.flow.control.Tee
					-name
					name
					-tee
						adams.flow.transformer.BaseName
							-remove-extensions
							true
					-tee
						adams.flow.transformer.SetVariable
							-var-name
							name
			-actor
				adams.flow.transformer.ImageReader
					-reader
						adams.data.io.input.JAIImageReader
			-actor
				adams.flow.control.Tee
					-tee
						adams.flow.sink.CallableSink
							-callable
							DisplayPanelManager
	-actor
		adams.flow.transformer.BufferedImageMultiImageOperation
			-operation
				adams.data.image.multiimageoperation.Median
	-actor
		adams.flow.transformer.SetVariable
			-var-name
			name
			-var-value
			median
	-actor
		adams.flow.sink.CallableSink
			-callable
			DisplayPanelManager
	-flow-execution-listener
		adams.flow.execution.NullListener

