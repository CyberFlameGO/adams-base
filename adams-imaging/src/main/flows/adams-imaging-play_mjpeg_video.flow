adams.flow.control.Flow
	-annotation
	Displays the images obtained from a MJPEG movie file.
	-actor
		adams.flow.source.SelectFile
			-stop-if-canceled
			true
			-file-chooser-title
			Select MJPEG file
			-extension
			mjpeg
	-actor
		adams.flow.transformer.MjpegImageSequence
			-output-type
			BOTH
	-actor
		adams.flow.control.Sleep
			-interval
			20
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					frame
					-actor
						adams.flow.transformer.GetArrayElement
					-actor
						adams.flow.sink.ImageViewer
							-name
							Frame
							-short-title
							true
							-writer
								adams.gui.print.NullWriter
							-selection-processor
								adams.gui.visualization.image.selection.NullProcessor
							-image-overlay
								adams.gui.visualization.image.NullOverlay
			-branch
				adams.flow.control.Sequence
					-name
					gui image
					-actor
						adams.flow.transformer.GetArrayElement
							-index
							2
					-actor
						adams.flow.sink.ImageViewer
							-name
							GUI Image
							-short-title
							true
							-x
							-3
							-writer
								adams.gui.print.NullWriter
							-selection-processor
								adams.gui.visualization.image.selection.NullProcessor
							-image-overlay
								adams.gui.visualization.image.NullOverlay
	-flow-execution-listener
		adams.flow.execution.NullListener

