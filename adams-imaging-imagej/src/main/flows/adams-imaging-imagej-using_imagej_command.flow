adams.flow.control.Flow -annotation "Runs the \\\"add noise\\\" ImageJ command on the image\\npassing through. The original and the processed \\nimage get displayed together.\\n\\nNote: the commands and options can be obtained\\nfrom ImageJ when recording Macros as Plugin." -flow-execution-listener adams.flow.execution.NullListener
 adams.flow.standalone.CallableActors
  adams.flow.sink.DisplayPanelManager -width 800 -height 600 -provider "adams.flow.sink.ImageViewer -writer adams.gui.print.NullWriter -selection-processor adams.gui.visualization.image.selection.NullProcessor -image-overlay adams.gui.visualization.image.NullOverlay" -entry-name-variable name
 adams.flow.source.FileSupplier -file ${FLOWS}/data/3666455665_18795f0741.jpg
 adams.flow.transformer.ImageJReader
 adams.flow.control.Tee
  adams.flow.transformer.Convert -conversion adams.data.conversion.ImageJToBufferedImage
  adams.flow.transformer.SetVariable -var-name name -var-value original
  adams.flow.sink.CallableSink -callable DisplayPanelManager
 adams.flow.transformer.ImageJTransformer -transformer adams.data.imagej.transformer.Command
 adams.flow.transformer.SetVariable -var-name name -var-value tranformed
 adams.flow.sink.CallableSink -callable DisplayPanelManager
