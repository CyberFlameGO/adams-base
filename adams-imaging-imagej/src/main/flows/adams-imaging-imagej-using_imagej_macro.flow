adams.flow.control.Flow -annotation "Loads images from disk and displays the originals\\nand ones transformed using a pre-recorded ImageJ macro\\napplying a bandpass filter." -flow-execution-listener adams.flow.execution.NullListener
 adams.flow.source.FileSystemSearch -search "adams.flow.source.filesystemsearch.LocalFileSearch -directory ${FLOWS}/data -regexp .*.jpg"
 adams.flow.control.Tee -name "extract name"
  adams.flow.transformer.BaseName
  adams.flow.transformer.SetVariable -var-name name
 adams.flow.transformer.ImageJReader
 adams.flow.control.Branch
  adams.flow.control.Sequence -name fft
   adams.flow.transformer.ImageJTransformer -transformer "adams.data.imagej.transformer.Macro -commands \"run(\\\\\\\"Bandpass Filter...\\\\\\\", \\\\\\\"filter_large=40 filter_small=3 suppress=None tolerance=5 autoscale saturate\\\\\\\");\""
   adams.flow.sink.DisplayPanelManager -name FFT -short-title true -width 700 -height 550 -provider "adams.flow.sink.ImageViewer -writer adams.gui.print.NullWriter -selection-processor adams.gui.visualization.image.selection.NullProcessor -left-click-processor adams.gui.visualization.image.leftclick.NullProcessor -image-overlay adams.gui.visualization.image.NullOverlay" -entry-name-variable name
  adams.flow.control.Sequence -name original
   adams.flow.sink.DisplayPanelManager -name Original -short-title true -width 700 -height 550 -x -3 -provider "adams.flow.sink.ImageViewer -writer adams.gui.print.NullWriter -selection-processor adams.gui.visualization.image.selection.NullProcessor -left-click-processor adams.gui.visualization.image.leftclick.NullProcessor -image-overlay adams.gui.visualization.image.NullOverlay" -entry-name-variable name