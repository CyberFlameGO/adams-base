adams.flow.control.Flow -annotation "Loads images from disk, processes them and \\nturns them into a CSV file.\\nThe final images are displayed as well." -flow-execution-listener adams.flow.execution.NullListener
 adams.flow.source.FileSystemSearch -search "adams.flow.source.filesystemsearch.LocalFileSearch -directory ${FLOWS}/data -regexp .*.jpg"
 adams.flow.transformer.ImageJReader
 adams.flow.transformer.ImageJTransformer -transformer adams.data.imagej.transformer.Gray8
 adams.flow.transformer.ImageJTransformer -name ImageJTransformer-1 -transformer "adams.data.imagej.transformer.Resize -width 50 -height 50"
 adams.flow.control.Tee -name "display image"
  adams.flow.transformer.Convert -conversion adams.data.conversion.ImageJToBufferedImage
  adams.flow.sink.DisplayPanelManager -x -3 -provider "adams.flow.sink.ImageViewer -writer adams.gui.print.NullWriter -selection-processor adams.gui.visualization.image.selection.NullProcessor -left-click-processor adams.gui.visualization.image.leftclick.NullProcessor -image-overlay adams.gui.visualization.image.NullOverlay"
 adams.flow.transformer.ImageJFeatureGenerator -algorithm "adams.data.imagej.features.Pixels -converter \"adams.data.featureconverter.SpreadSheet -data-row-type adams.data.spreadsheet.DenseDataRow -spreadsheet-type adams.data.spreadsheet.DefaultSpreadSheet\""
 adams.flow.sink.SpreadSheetFileWriter -output ${FLOWS}/output/flattened.csv -writer adams.data.io.output.CsvSpreadSheetWriter