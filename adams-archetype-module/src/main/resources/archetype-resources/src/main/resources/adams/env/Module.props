# Contains information about the module.
#
# Author: FracPete (fracpete at waikato dot ac dot nz)
# Version: $Revision$

# The name of the module
Name=${artifactId}

# The version of the module
Version=${project.version}

# The build timestamp
BuildTimestamp=${timestamp}

# A short description of the module
Description=

# The author of the module
Author=

# The author's organization
Organization=

# The logo of the module (leave empty for default; 48x48 pixel).
# If the logo is provided without path, it is assumed to be located in the
# adams/gui/images sub-directory of the module.
Logo=

# Whether this module is a meta-module and does not contain actual code etc.
# (false|true)
MetaModule=false
